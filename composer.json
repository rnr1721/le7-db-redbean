{
    "name": "rnr1721/le7-db-redbean",
    "description": "Redbean PHP that allow easy use of this ORM in any PHP project",
    "keywords": ["le7", "le7-framework", "framework", "redbean", "database", "component"],
    "authors": [
        {
            "name": "Eugeny G",
            "email": "rnr1721@gmail.com",
            "homepage": "https://xhamster.biz.ua",
            "role": "Developer"
        }
    ],
    "minimum-stability": "stable",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Core\\Database\\Redbean\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TestsModel\\": "tests/TestsModel"
        }
